<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="156.0" prefHeight="190.0" prefWidth="300.0" stylesheets="@bootstrap3.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursrab.controllers.NewJournalController">
   <children>
      <VBox prefHeight="154.0" prefWidth="300.0" AnchorPane.bottomAnchor="46.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Выберете ученика:">
               <padding>
                  <Insets left="5.0" />
               </padding>
            </Label>
            <ComboBox fx:id="cmBoxStud" onAction="#onChangedStud" prefHeight="25.0" prefWidth="321.0" />
            <Label text="Выберите предмет:">
               <padding>
                  <Insets left="5.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="cmBoxSubj" onAction="#onChangedSubj" prefHeight="25.0" prefWidth="303.0">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </ComboBox>
            <Label text="Введите оценку:">
               <padding>
                  <Insets left="5.0" top="10.0" />
               </padding>
            </Label>
            <ComboBox fx:id="cmBoxScore" onAction="#onChangedScore" prefWidth="150.0" />
         </children>
      </VBox>
      <HBox prefHeight="33.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onOk" text="Ok">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onCancel" text="Отменить">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
